{
  "name": "REDIRECTOR",
  "version": "1",
  "background": {
    "scripts": [
      "popup.js",
      "background.js"
  ]
},
"manifest_version": 2,
"browser_action": {
"default_icon": "icon.jpg",
"default_popup": "popup.html"
},
"permissions": ["webRequest","webRequestBlocking", "http://*/*", "https://*/*"]
}